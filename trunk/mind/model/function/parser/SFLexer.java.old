          }
        case 33: break;
        case 10: 
          { semVal = yytext(); return token = RPAR;
          }
        case 34: break;
        case 20: 
          { semVal = yytext(); return token = FLOATVARTSO;
          }
        case 35: break;
        case 18: 
          { semVal = yytext(); return token = FLOWVARTSO;
          }
        case 36: break;
        case 19: 
          { semVal = yytext(); return token = INTVARTSO;
          }
        case 37: break;
        case 13: 
          { semVal = yytext(); return token = DIV;
          }
        case 38: break;
        case 5: 
          { semVal = "G"; return token = OP;
          }
        case 39: break;
        case 7: 
          { /* ignore space */
          }
        case 40: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            zzDoEOF();
              { if (yymoreStreams()) { yypopStream(); } else return ENDINPUT; }
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


